# アプリケーション設定
APP_NAME=MyScheduler API
APP_VERSION=1.0.0
DEBUG=false

# データベース
DATABASE_URL=sqlite:///./data/jobs.db

# スケジューラー
TIMEZONE=Asia/Tokyo
JOB_MAX_INSTANCES=1
JOB_COALESCE=true
JOB_MISFIRE_GRACE_TIME=30

# HTTP
HTTP_TIMEOUT=30.0
MAX_RETRIES=3
RETRY_BACKOFF=1.0

# サーバー
HOST=0.0.0.0
PORT=8000
WORKERS=1

# ログ
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s