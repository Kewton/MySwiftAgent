# MySwiftAgent Environment Variables Example
# Copy this file to .env and fill in your actual values

# ============================================================================
# API Keys
# ============================================================================

# OpenAI API Key (for GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key (for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# ============================================================================
# Service Configuration
# ============================================================================

# Database URLs
JOBQUEUE_DB_URL=sqlite+aiosqlite:///./data/jobqueue.db

# Service URLs (for development)
JOBQUEUE_API_URL=http://localhost:8001
MYSCHEDULER_API_URL=http://localhost:8002
EXPERTAGENT_API_URL=http://localhost:8003
GRAPHAISERVER_API_URL=http://localhost:8004

# ============================================================================
# Environment Settings
# ============================================================================

# Timezone
TZ=Asia/Tokyo

# Node Environment (production, development)
NODE_ENV=production

# Python Path
PYTHONPATH=/app

# ============================================================================
# Optional: Additional Settings
# ============================================================================

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Enable Debug Mode (true, false)
DEBUG=false
